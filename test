from docx import Document

# Open the Word document
doc_path = 'document.docx'
doc = Document(doc_path)

# Find the row that contains "Image Name : Version"
target_row = None
for table in doc.tables:
    for row in table.rows:
        cell_text = ' '.join(cell.text for cell in row.cells)
        if "Image Name : Version" in cell_text:
            target_row = row
            break
    if target_row:
        break

# Extract the data from the target row
data = [cell.text for cell in target_row.cells]

# Print the extracted data
print(data)
